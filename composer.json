{
    "name": "andydune/task-lock",
    "description": "Usefull for control tasks to avoid parallel job. It support MongoDB or files.",
    "license": "MIT",
    "keywords": [
        "php",
        "cron",
        "task"
    ],
    "authors": [
        {
            "name": "Andrey Ryzhov",
            "email": "info@rznw.ru"
        }
    ],
    "homepage": "https://github.com/AndyDune/ConditionalExecution",
    "autoload": {
        "psr-4": {
            "AndyDune\\TaskLock\\": "src/"
        }
    },
    "require": {
        "php": "^7.1",
        "andydune/conditional-execution": "^1"
    },
    "require-dev": {
